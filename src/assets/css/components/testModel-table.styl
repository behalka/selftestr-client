.test-models
  width 100%
  border 1px solid $color-grey
  margin 1.5em 0
  tr
    border-bottom 1px solid $color-grey
  &__header
    set-font-size 18px
    font-weight bold
  &__header > td
  &__item > td
    border-right 1px solid $color-grey
    vertical-align middle
    padding 1em
  &__btn
    margin-right 0.4em 